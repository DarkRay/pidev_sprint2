    <body class="skin-black">
        <div class="wrapper">
            
            <header class="main-header">
                <!-- Logo -->
                <a href="" class="logo"><img src="{{ asset('bundles/sprint2realestateadmin/img/logo.png') }}"/></a>
                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top" role="navigation">
                    <!-- Sidebar toggle button-->
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            
                                
                            <!-- User Account: style can be found in dropdown.less -->
                            <li class="dropdown user user-menu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    {% block Image1 %}{% endblock %}
                                    
                                    <span class="hidden-xs">{{ app.session.get('compte').nom}} {{ app.session.get('compte').prenom}}</span>
                                </a>
                                <ul class="dropdown-menu">
                                    <!-- User image -->
                                    <li class="user-header">
                                        {% block Image2 %}{% endblock %}
                                        
                                        <p>
                                            {{ app.session.get('compte').nom}} {{ app.session.get('compte').prenom}}
                                            <small>{% if app.session.get('compte').privilege==7 %} Super Admin{% endif %}</small>
                                        </p>
                                    </li>
                                    <!-- Menu Body -->
                                        
                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        
                                        <div class="pull-right">
                                            <a href="logout" class="btn btn-default btn-flat">Sign out</a>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="main-sidebar">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">
                    <!-- Sidebar user panel -->
                    <div class="user-panel">
                        <div class="pull-left image">
                        {% block Image %}{% endblock %}             
                    </div>
                    <div class="pull-left info">
                        <p>{{ app.session.get('compte').nom}} {{ app.session.get('compte').prenom}}</p>
                            
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <!-- search form -->
                
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    <li {% block DashActive %}{% endblock %}>
                        <a href="./">
                            <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                        </a> 
                    </li>
                    <li {% block MailboxActive %}{% endblock %}>
                        <a href="MailBox">
                            <i class="fa fa-envelope"></i> <span>Mailbox</span>
                        {% block MailboxEtra %}{% endblock %}
                    </a>
                {% block MailboxEtra2 %}{% endblock %}
            </li>
            <li {% block adminsActive %}{% endblock %}>
              <a href="Admins">
                <i class="fa fa-user"></i> <span>Liste des Admins</span>
              </a>
            </li>
            <li {% block clientsActive %}{% endblock %}> 
                <a href="Clients">
                    <i class="fa fa-users"></i> <span>Liste des clients</span>
                </a>
            </li>
                
            <li {% block gerantsActive %}{% endblock %}>
                <a href="Gerants">
                    <i class="fa fa-user"></i> <span>Liste des g√©rants</span>
                </a>
            </li>
                
            <li {% block AganceActive %}{% endblock %}> 
                <a href="Agences">
                    <i class="fa fa-suitcase"></i> <span>Liste des Agence</span>
                </a>
            </li>
                
            <li {% block offreActive %}{% endblock %}>
                <a href="Offres">
                    <i class="fa fa-shopping-cart"></i> <span>Liste des offre</span>
                </a>
            </li>
            <li {% block StatActive %}{% endblock %}>
              <a href="chartPie">
                <i class="fa fa-pie-chart"></i> <span>Statistiques</span>
              
              {% block PieChart %}{% endblock %}
              </a>
              {% block LineChart %}{% endblock %}
            </li>
            
            <li {% block ContactActive %}{% endblock %}>
              <a href="sendMail">
                <i class="fa fa-envelope-o"></i> <span>Contacter</span>
              </a>
            <li {% block ArchiveActive %}{% endblock %}>
                <a href="listArchive">
                    <i class="fa fa-folder"></i> <span>Archive</span>
                </a>
            </li>
                
                
    </section>
    <!-- /.sidebar -->
</aside>
    
